package automobily;

public enum Barva {
    BILA("bílá"), ZLUTA("žlutá"), CERNA("černá"), MODRA("modrá"), CERVENA("červená");

    private String nazev;

    private Barva(String nazev) {
        this.nazev = nazev;
    }

    public String getNazev() {
        return nazev;
    }

    @Override
    public String toString() {
        return nazev;
    }
}
