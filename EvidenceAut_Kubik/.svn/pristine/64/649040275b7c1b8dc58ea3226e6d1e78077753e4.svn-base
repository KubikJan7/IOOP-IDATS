package automobily;

public class Dodavka extends Vozidlo{
private final int uzitnaHmotnost;
private final int objemUloznehoProstoru;

    public Dodavka(int uzitnaHmotnost, int objemUloznehoProstoru, String vyrobce, int SPZ, int maxRychlost, boolean vyhrivanaSedadla) {
        super(vyrobce, SPZ, maxRychlost, vyhrivanaSedadla);
        this.uzitnaHmotnost = uzitnaHmotnost;
        this.objemUloznehoProstoru = objemUloznehoProstoru;
    }

    public int getUzitnaHmotnost() {
        return uzitnaHmotnost;
    }

    public int getObjemUloznehoProstoru() {
        return objemUloznehoProstoru;
    }

    @Override
    public String toString() {
        return "Dodavka{" + super.toString() + "uzitnaHmotnost=" + uzitnaHmotnost + ", objemUloznehoProstoru=" + objemUloznehoProstoru + '}';
    }


}
